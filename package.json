{
  "name": "tiny-swiper",
  "version": "2.1.2",
  "description": "Ingenious JavaScript Carousel powered by wonderful plugins with native-like experience for the web.",
  "main": "lib/index.min.js",
  "module": "lib/index.esm.js",
  "types": "types/index.esm.d.ts",
  "browser": "lib/index.min.js",
  "jsdelivr": "lib/index.min.js",
  "unpkg": "lib/index.min.js",
  "repository": "https://github.com/joe223/tiny-swiper.git",
  "author": "joe223",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup -wc",
    "build": "cross-env NODE_ENV=production rollup -c && tsc src/*.ts --declaration --emitDeclarationOnly --outDir types",
    "test": "jest",
    "test:e2e:local": "cross-env TEST_MODE=local npm run test:e2e",
    "report": "nyc npm run test && nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint . --config .eslintrc.js",
    "lint:fix": "eslint . --fix --config .eslintrc.js"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-typescript": "^8.0.0",
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "coveralls": "^3.0.7",
    "cross-env": "^6.0.3",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^3.1.0",
    "jest": "^25.1.0",
    "jest-puppeteer": "^4.4.0",
    "nyc": "^15.1.0",
    "rollup": "^2.34.2",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^25.2.1",
    "ts-mocha": "^6.0.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "browserslist": [
    "Android >= 7",
    "IOS >= 11",
    "Safari >= 11",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
  "keywords": [
    "swiper",
    "swipe",
    "slider",
    "touch",
    "ios",
    "mobile",
    "tiny-swiper",
    "tinyswiper",
    "carousel",
    "gallery",
    "plugin"
  ],
  "nyc": {
    "report-dir": "./coverage",
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "__test__",
      "node_modules",
      "src/index.esm.js",
      "src/index.full.js"
    ],
    "watermarks": {
      "lines": [
        70,
        85
      ],
      "functions": [
        70,
        85
      ],
      "branches": [
        70,
        85
      ],
      "statements": [
        70,
        85
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "dependencies": {
    "typescript": "^4.1.3"
  }
}
